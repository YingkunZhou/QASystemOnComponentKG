{ "_id" : { "$oid" : "5cc93092831b973d657f0f6d" }, "名称" : "F9F战斗机", "产国" : "美国", "图片" : "http://images.huanqiu.com/sarons/2013/08/d5ec8948682e84e6e45fef390a894dd4.jpg", "简介" : "\nF9F黑豹是格鲁曼所生产的第一架喷气式战斗机，同时也是美国海军航空母舰上成功布署的喷气式舰载战斗轰炸机之一。黑豹是朝鲜战争中最广泛使用的美国海军战斗机，一共飞行了78000架次，并且是美国海军在该战争中第一个取得空对空战果的机种，击落了北朝鲜的雅科夫列夫雅克-9战斗机。\n", "首飞时间" : "1947年11月24日", "服役时间" : "1958年", "研发单位" : "格鲁曼公司", "生产单位" : "格鲁曼公司", "气动布局" : "平直翼", "发动机数量" : "单发", "飞行速度" : "亚音速", "乘员" : "1人", "机长" : "10.06米", "翼展" : "11.98米", "机高" : "3.74米", "空重" : "4220千克", "发动机" : "普-惠J42-P-6A发动机", "最大起飞重量" : "8492千克", "最大飞行速度" : "925千米每小时", "最大航程" : "2100千米", "大类" : "飞行器", "类型" : "战斗机" }
