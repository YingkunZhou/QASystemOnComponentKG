{ "_id" : { "$oid" : "5cc93091831b973d657f0f66" }, "名称" : "阵风战斗机", "产国" : "法国", "图片" : "http://images.huanqiu.com/sarons/2013/07/38747b38b540dd34254fea4713256a1a.jpg", "简介" : "\n阵风战斗机，或音译为拉斐尔战斗机，是法国达索飞机制造公司设计制造的双发、三角翼、高机动性、多用途第四代半战斗机。阵风虽然没有采用像F-22战斗机第五代战斗机的“外形设计隐身”、“矢量推力”、“超音速巡航”等技术，但比起现役的第四代战斗机，其综合作战性能仍有很大提高，发展潜力巨大。\n", "首飞时间" : "1986年7月4日", "服役时间" : "2000年12月4日", "研发单位" : "法国达索公司", "气动布局" : "鸭式", "发动机数量" : "双发", "飞行速度" : "超音速", "乘员" : "2人", "机长" : "15.3米", "翼展" : "10.8米", "机高" : "5.3米", "空重" : "9500千克", "发动机" : "两台SNECMA M88-2涡轮风扇发动机", "最大起飞重量" : "24500千克", "最大飞行速度" : "2205千米每小时", "最大航程" : "3700千米", "大类" : "飞行器", "类型" : "战斗机" }
