{ "_id" : { "$oid" : "5cc93099831b973d657f0fa5" }, "名称" : "格鲁曼F2F/F3F", "产国" : "美国", "图片" : "", "简介" : "\n受FF-1成功的鼓舞，格鲁曼飞机制造厂顺理成章地在同一生产线上开始了单翼战斗机的设计——金属蒙皮机身、织物蒙皮的金属机翼、大功率星形发动机（一台普-惠“双小黄蜂”发动机），以及收放式起落架。该设计被格鲁曼飞机制造厂命名为G-8，后改为F2F，于1932年6月提交给美国海军，同年11月接到了生产一架XF2F原型机的合同。\n", "首飞时间" : "1933年10月18日（XF2F-1）", "退役时间" : "1941年", "研发单位" : "格鲁曼公司", "生产单位" : "格鲁曼公司", "气动布局" : "双翼", "发动机数量" : "单发", "飞行速度" : "亚音速", "乘员" : "1人", "机长" : "7.07米", "翼展" : "9.75米", "机高" : "2.84米", "空重" : "1478千克", "发动机" : "莱特R-1820-22“旋风”发动机", "最大起飞重量" : "2042千克", "最大飞行速度" : "425千米每小时", "最大航程" : "1328千米", "大类" : "飞行器", "类型" : "战斗机" }
